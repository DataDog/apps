    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/postmate": "^1.5.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "serve-static": "^1.14.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.1.1",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "3.11.1"
  },
  "typedocMain": "src/index.ts"
}